<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yeungeek.dagger">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.yeungeek.dagger.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.yeungeek.dagger.ui.InjectActivity" />
        <activity android:name="com.yeungeek.dagger.ui.ProvidesActivity" />
        <activity android:name="com.yeungeek.dagger.ui.QualifierActivity" />
        <activity android:name="com.yeungeek.dagger.ui.ScopeActivity" />
        <activity android:name="com.yeungeek.dagger.ui.SingletonActivity" />
        <activity android:name="com.yeungeek.dagger.ui.LazyActivity" />
        <activity android:name="com.yeungeek.dagger.ui.NormalSetActivity" />
        <activity android:name="com.yeungeek.dagger.ui.MapActivity" />
    </application>
</manifest>